<% provide :title, @user.name %>
<div class="row", id="main">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
        <%= link_to "", user_user_courses_path(@user), class: "btn btn-default glyphicon glyphicon-th-list" %>

    </section>
  </aside>
</div>
